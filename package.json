{
  "name": "splittypie",
  "version": "1.0.0",
  "description": "Split your expenses with ease",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "pretest": "node ./node_modules/.bin/firebase-server -p 5005 &",
    "test": "ember test",
    "posttest": "pkill -f 'node ./node_modules/.bin/firebase-server' &",
    "deploy": "rm -rf dist/ && ember build && firebase use dev && firebase deploy",
    "deploy:prod": "rm -rf dist/ && ember build --environment=production && firebase use prod && firebase deploy",
    "deploy:staging": "rm -rf dist/ && ember build --environment=staging && firebase use staging && firebase deploy",
    "deploy:offline": "rm -rf dist/ && ember build --environment=offline && firebase use offline && firebase deploy",
    "seed": "firebase use dev && ./get-seed-json.js default | firebase database:set / -y",
    "seed:staging": "firebase use staging && ./get-seed-json.js default | firebase database:set / -y"
  },
  "repository": "",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Tomasz Subik",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.7.1",
    "broccoli-asset-rev": "^2.4.5",
    "broccoli-serviceworker": "~0.1.6",
    "dotenv": "^4.0.0",
    "ember-ajax": "2.5.6",
    "ember-cli": "2.11.1",
    "ember-cli-accounting": "2.0.0",
    "ember-cli-app-version": "2.0.1",
    "ember-cli-babel": "^5.1.10",
    "ember-cli-content-security-policy": "0.6.0",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-deprecation-workflow": "0.2.3",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-htmlbars": "^1.1.0",
    "ember-cli-htmlbars-inline-precompile": "^0.3.5",
    "ember-cli-ic-ajax": "1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-inline-content": "0.4.1",
    "ember-cli-moment-shim": "3.0.1",
    "ember-cli-qunit": "^3.0.1",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sass": "6.1.1",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-test-model-waiter": "0.1.1",
    "ember-cli-uglify": "^1.2.0",
    "ember-cp-validations": "3.2.3",
    "ember-data": "2.11.3",
    "ember-data-fixture-adapter": "git://github.com/emberjs/ember-data-fixture-adapter#2-0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.5",
    "ember-index": "0.2.9",
    "ember-inline-svg": "0.1.7",
    "ember-load-initializers": "^0.6.3",
    "ember-localforage-adapter": "git://github.com/Flexberry/ember-localforage-adapter.git#master",
    "ember-notify": "5.2.0",
    "ember-pikaday": "2.2.1",
    "ember-qunit-nice-errors": "1.1.2",
    "ember-resolver": "2.1.1",
    "ember-route-action-helper": "2.0.2",
    "ember-side-menu": "git://github.com/tsubik/ember-side-menu#master",
